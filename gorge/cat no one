    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        
        public void actionPerformed(ActionEvent e) {
        if (() == registerButton) {
            if (acceptTerms.getState()) {
                String name = nameField.getText();
                String mobile = mobileField.getText();
                String address = addressField.getText();
                String dob = dobField.getText();
                
                // Here you can add code to handle the registration logic
                System.out.println("Registered: " + name + ", " + mobile + ", " + address + ", " + dob);
            } else {
                System.out.println("You must accept the terms and conditions.");
            }
        }
    }
    }                                        

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
 if (() == resetButton) {
            // Clear all fields and uncheck the checkbox
            nameField.setText("");
            mobileField.setText("");
            addressField.setText("");
            dobField.setText("");
            acceptTerms.setState(false);
        }
    }                                        

